#include <iostream>

int main()
{
   float Quantidade, Resultado;
   printf("\t Digite a Quantidade de MaÃ§as \n");
   scanf("%f",&Quantidade);
   if (Quantidade<=12){
       Resultado=Quantidade*0.25;
          printf("Resultado = %.1f ",Resultado);
  
   }else{
   Resultado=Quantidade*0.30;
