#include <iostream>
using namespace std;

int main()
{
    float num[10];
    
    for (int cont = 0; cont < 10; cont++)
    {
        cout << "Valor " << cont + 1 << ": ";
        cin >> num[cont];
    }

    cout << "\n";
    for (int cont = 9; cont >= 0; cont--)
    {
        cout << "Valor " << 10 - cont << ": " << num[cont] << endl;
    }

    return 0;
}
